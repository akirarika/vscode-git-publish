{
  "name": "vscode-git-publish",
  "displayName": "vscode-git-publish",
  "publisher": "akirarika",
  "description": "",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.56.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akirarika/vscode-git-publish.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-git-publish.main"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-git-publish.main",
        "title": "Git Publish",
        "icon": {
          "light": "./icons/anchor-light.svg",
          "dark": "./icons/anchor-dark.svg"
        }
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "vscode-git-publish.main",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.56.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.19.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
